// 将键绑定放入此文件中以覆盖默认值
[
    {
        "key": "alt+s",
        "command": "markdown.showPreviewToSide",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k v",
        "command": "-markdown.showPreviewToSide",
        "when": "editorFocus"
    },
    {
        "key": "alt+`",
        "command": "workbench.action.toggleActivityBarVisibility"
    },
    {
        "key": "alt+1",
        "command": "workbench.action.switchWindow"
    },
    {
        "key": "ctrl+shift+s",
        "command": "workbench.action.files.revealActiveFileInWindows"
    },
    {
        "key": "ctrl+k r",
        "command": "-workbench.action.files.revealActiveFileInWindows"
    }
]